extends base.pug

block scripts 
    script 
        include ../static/script.js
block style 
    style 
        include ../static/style.css
        include ../static/styleContact.css
block content
    h1 Contact Us
    form(action='/contact' method='post')
        input(type='text' name='myName' id='name' class='myInput' placeholder='Enter Your Name')
        input(type='number' name='myContact' id='contact' class='myInput' placeholder='Enter Your Contact')
        input(type='email' name='myEmail' id='email' class='myInput' placeholder='Enter Your Email')
        textarea(name='myAbout' id='about' class='myInput' placeholder='Enter About Yourself')
        button#btn Submit Now